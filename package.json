{
  "name": "nodeaapi",
  "version": "1.0.0",
  "description": "node API with babel",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js --require dotenv/config --exec babel-node --presets env",
    "dbmigrate": "babel-node ./api/database/migrations --require dotenv/config --presets env"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validation": "^1.0.2",
    "joi": "^13.3.0",
    "mongoose": "^5.1.0",
    "mysql2": "^1.5.3",
    "sequelize": "^4.37.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.17.4"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "sourceMaps": true,
    "retainLines": true
  }
}
